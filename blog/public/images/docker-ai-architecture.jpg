    <line x1="230" y1="160" x2="300" y2="120"/>
    <line x1="230" y1="160" x2="300" y2="220"/>
    <line x1="230" y1="160" x2="300" y2="320"/>
    <line x1="440" y1="220" x2="520" y2="160"/>
    <line x1="400" y1="420" x2="370" y2="360"/>
