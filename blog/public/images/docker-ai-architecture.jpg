  <rect x="300" y="280" width="140" height="80" fill="#dc2626" stroke="#b91c1c" stroke-width="2" rx="8"/>
  <text x="370" y="310" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="12" font-weight="bold">CPU-2</text>
