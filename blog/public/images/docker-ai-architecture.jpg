  <text x="140" y="130" text-anchor="middle" fill="#ffffff" font-family="Arial, sans-serif" font-size="14" font-weight="bold">GPU Container</text>
  <text x="140" y="150" text-anchor="middle" fill="#bfdbfe" font-family="Arial, sans-serif" font-size="11">llama-gpu</text>
  <text x="140" y="170" text-anchor="middle" fill="#bfdbfe" font-family="Arial, sans-serif" font-size="11">RTX 5090</text>
  <text x="140" y="190" text-anchor="middle" fill="#bfdbfe" font-family="Arial, sans-serif" font-size="11">CUDA 12.9</text>
  
